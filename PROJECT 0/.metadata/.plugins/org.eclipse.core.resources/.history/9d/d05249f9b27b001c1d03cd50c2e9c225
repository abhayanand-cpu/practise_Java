import java.util.Scanner;
public class A3Q10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter today's day: ");
		int d = sc.nextInt();
		System.out.print("Enter the number of days elapsed since today: ");
		int e = sc.nextInt();
		e = (e>6) ? e/7-1:e+1;
		d = d==0 ? System.out.println("Sunday")
			::d==1 ? System.out.println("Monday")
			::d==2 ? System.out.println("Tuesday")
			::d==3 ? System.out.println("Wednesday")
			::d==4 ? System.out.println("Thrusday")
			::d==5 ? System.out.println("Friday")
			::d==6 ? System.out.println("Saturday"):;
				
		
	}
	
	
}

