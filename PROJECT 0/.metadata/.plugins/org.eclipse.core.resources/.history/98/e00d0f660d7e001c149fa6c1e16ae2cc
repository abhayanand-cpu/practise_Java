import java.util.Scanner;
public class A3Q_12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter three integers: ");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		if( a>b && a>c )
			System.out.println(a);
		sc.close();
	}

}
