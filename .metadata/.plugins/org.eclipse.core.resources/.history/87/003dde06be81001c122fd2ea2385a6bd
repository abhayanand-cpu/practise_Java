import java.util.Scanner;
public class A4Q_7 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		
		int i=Integer.parseInt(s); 
		System.out.println(i);
		sc.close();
	}

}
