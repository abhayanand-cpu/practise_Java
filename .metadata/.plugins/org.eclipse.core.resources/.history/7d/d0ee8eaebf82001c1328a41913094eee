import java.util.Scanner;
public class A4Q_11_2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 1; i<=n; i++) {
			int j;
			for (  j = 1; j<=n; j++ ) {
				if ( j<=n-i )
					System.out.print(" ");
				 
					
			}
			System.out.print(j);
			System.out.println();
		}
		sc.close();
	}

}
