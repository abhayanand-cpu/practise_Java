import java.util.Scanner;
public class A4Q_5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter range: ");
		int n = sc.nextInt();
		
		for (int i = 1; i<=n; i++) {
			int sum = 0;
			if ( i%2==0 ) {
				System.out.print(i);
				if (i!=n)
				System.out.print("+");
				sum = sum+i;
			}
		}
		System.out.println();
		
		for (int i = 1; i<=n; i++) {
			int product = 1;
			if ( i%2!=0) {
				System.out.print(i);
				if (i!=n)
				System.out.print("*");
				product = product*i;
			}
		}
		
		sc.close();
	}

}
