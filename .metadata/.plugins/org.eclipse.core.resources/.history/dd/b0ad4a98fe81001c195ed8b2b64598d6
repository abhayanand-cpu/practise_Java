import java.util.*;
public class A3Q_19 {

 public static void main(String[] args) {
  // TODO Auto-generated method stub
  Scanner sc = new Scanner(System.in);
  System.out.println("Gender(M or F): ");
  char g = sc.next().charAt(0);
  int gender = (int)g;
  System.out.println(gender);
  System.out.println("First name: ");
  String f = sc.next();
  System.out.println("Last name: ");
  String l = sc.next();
  System.out.println("Age: ");
  int a = sc.nextInt();
  if(gender == 70) {
   if(a >= 20) {
    System.out.println("Are you married, " + f + " (y or n)?");
    char m = sc.next().charAt(0);
    if((int)m==121)
     System.out.println("Then I shall call you Mrs. " + f + " " + l + ".");
    else System.out.println("Then I shall call you Ms. " + f + " " + l + ".");
   }
   else System.out.println("Then I shall call you " + f + " " + l + ".");
  }
  else {
   if(a >= 20) System.out.println("Then I shall call you Mr. " + f + " " + l + ".");
   else System.out.println("Then I shall call you " + f + " " + l + ".");
  }
  sc.close();
 }

}
