import java.util.Scanner;
public class A4Q_5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter range :");
		int n = sc.nextInt();
		int sum = 0;
		for (int i = 1; i<=n; i++) {
			
			if ( i%2==0 ) {
				System.out.print(i+" + "+" = "+sum);
				sum = sum+i;
			}
			
		}
		
		sc.close();
	}

}
