import java.util.Scanner;
public class A4Q_11_4 {
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int k = 1;
		for (int i = 1; i<=n; i++) {
			for ( int j = 1;  j<=i; j++) {
//				if ( j<=n-i )
//					System.out.print(" ");
				System.out.print(k);
				k++;
			}
			System.out.println();
		}
		
	sc.close();
	}
			
			
}
		
	


