
public class A4Q_7 {

	public static void main(String[] args) {
		
		System.out.println("The sum of the squares of the first ten natural numbers is,");
		int sum1 = 0;
		int sum2 = 0;
		for (int i=1; i<=10; i++) {
			sum1 = sum1+(int)(Math.pow(i, 2));
			sum2 = (sum2+i);
		}
		System.out.println(sum1);
		System.out.println(Math.pow(sum2, 2));
	}

}

