import java.util.Scanner;
public class A3Q_16 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter your marks among 100: ");
	    int marks = sc.nextInt();
	    
	    switch (marks) {
	    case 1:
	    	int marks>=90
	    	System.out.println("Your grade: O");
	    	break;
	    }
		sc.close();
	}

}
